<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.expensetrack" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
    <name>Xpense Tracker</name>
    <description>Xpense Tracker - A cordova-vue project</description>
    <author email="tayotomichael18@gmail.com" href="https://github.com/Mica56">
        Mica56
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon density="ldpi" src="res/icon/android/app-logo-ldpi.png" />
    <icon density="mdpi" src="res/icon/android/app-logo-mdpi.png" />
    <icon density="hdpi" src="res/icon/android/app-logo-hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/app-logo-xhdpi.png" />
    <icon density="xxhdpi" src="res/icon/android/app-logo-xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icon/android/app-logo-xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
